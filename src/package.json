{
  "name": "src",
  "version": "1.0.0",
  "description": "CUE Dashboard",
  "author": "TobiasEmil",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:win": "build.sh",
    "deploy:win": "build.sh && firebase deploy --only functions,hosting",
    "deploy:stage:win": "build.sh && firebase use staging && firebase deploy --only functions,hosting",
    "deploy:dev:win": "build.sh && firebase use development && firebase deploy --only functions,hosting",
    "deploy:test:win": "build.sh && firebase use user-test && firebase deploy --only functions,hosting",
    "serve:win": "build.sh && firebase serve --only functions,hosting",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/vuetify": "^0.5.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase": "^5.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "nuxt": "^2.3.4",
    "nuxt-validate": "^0.1.2",
    "vuetify": "^1.4.1",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
